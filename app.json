{
  "expo": {
    "name": "ImagineCalculator",
    "slug": "imagine-calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FAFAF8"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FAFAF8"
      }
    },
    "web": {
      "bundler": "metro",
      "lang": "en",
      "themeColor": "#FAFAF8",
      "name": "ImagineCalculator - Imagine Your Crypto Future",
      "description": "Dream big with ImagineCalculator. See what your crypto portfolio could become with live prices, DCA scenarios, and what-if calculations."
    },
    "plugins": [
      "expo-font"
    ]
  }
}
